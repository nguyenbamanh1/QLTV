<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView20"
        android:layout_width="90dp"
        android:layout_height="28dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="Mã đăng ký:"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txthoten"
        android:layout_width="240dp"
        android:layout_height="28dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="9dp"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/textView21"
        app:layout_constraintTop_toBottomOf="@+id/txtmadk" />

    <TextView
        android:id="@+id/txtgioitinh"
        android:layout_width="240dp"
        android:layout_height="28dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="9dp"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/textView22"
        app:layout_constraintTop_toBottomOf="@+id/txthoten" />

    <TextView
        android:id="@+id/txtloaidk"
        android:layout_width="240dp"
        android:layout_height="28dp"
        android:layout_marginTop="7dp"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/textView23"
        app:layout_constraintTop_toBottomOf="@+id/txtgioitinh" />

    <TextView
        android:id="@+id/txtmadk"
        android:layout_width="240dp"
        android:layout_height="28dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="16dp"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/textView20"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="90dp"
        android:layout_height="28dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="9dp"
        android:text="Giới tính:"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView21" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="95dp"
        android:layout_height="28dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="7dp"
        android:text="Loại đăng ký:"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView22" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="90dp"
        android:layout_height="28dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="9dp"
        android:text="Họ tên:"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView20" />
</androidx.constraintlayout.widget.ConstraintLayout>